<div class="row" style="margin-left: 20px;">
 
    <div style="margin-left: 20px;">
        <div class="span9 page-header" style="margin-bottom: 10px;">
            <div class="span5"><h1>My Deck <small> (<%= count %>)</small></h1></div>
            <div class="span4" style="margin-left: 0"></div>
        </div>
        
        <% if (count > 2) { %>
        <ul class="nav nav-pills">
            <li>
                <a class="sort" data-sort="created_at" data-dir="asc" onclick="return false;" href="#">
                  Date Created 
                </a>
            </li>
            <li>
                <a class="sort" data-sort="year" data-dir="asc" onclick="return false;" href="#">
                  Model Year 
                </a>
            </li>
            <li>
                <a class="sort" data-sort="tt" data-dir="asc" onclick="return false;" href="#">
                  Total Time 
                </a>
            </li>                        
        </ul>
        <% } %>
    </div>
    
    <div id="airframes"></div>
    
</div>

<% if (pages > 1) { %>
<div class="row"  style="margin-left: 20px;" >
    <div class="span9">
        <div class="pagination pull-right">
          <ul>
            <li><a class="prev"  onclick="return false;" href="#">Prev</a></li>

            <% if (pages > 2) { %>
            <% for (var i = 1; i < pages + 1; i++) { %>
            <li><a class="page" rel="<%= i %>" onclick="return false;" href="#"><%= i %></a></li>
            <% } } %>

            <li><a class="next" onclick="return false;" href="#">Next</a></li>
          </ul>
        </div>
    </div>
</div>
<% } %> 

