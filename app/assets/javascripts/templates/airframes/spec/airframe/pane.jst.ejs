
<table class="table table-condensed">
<tbody>
  <tr>
    <td style="border-top: 0px; padding-top: 13px"><h4><small>Total Time</small></h4></td>
    <td style="border-top: 0px;">
      <input type="text" class="inline-edit number airframe-spec-field table-input-margin"
            value="<%= tt %>" name="tt"></input>
    </td>
    <td style="border-top: 0px;">&nbsp;</td>
    <td style="border-top: 0px; padding-top: 13px"><h4><small>Total Cycles</small></h4></td>
    <td style="border-top: 0px;">
      <input type="text" class="inline-edit number airframe-spec-field table-input-margin"
            value="<%= tc %>" name="tc"></input>
    </td>
  </tr>            
  <tr>
    <td style="border-top: 0px; padding-top: 13px"><h4><small>Year</small></h4></td>
    <td style="border-top: 0px;">
      <input type="text" class="inline-edit airframe-spec-field table-input-margin"
            value="<%= year %>" name="year"></input>
    </td>
    <td style="border-top: 0px;">&nbsp;</td>
    <td style="border-top: 0px; padding-top: 13px"><h4><small>Make</small></h4></td>
    <td style="border-top: 0px;">
      <input type="text" class="inline-edit airframe-spec-field table-input-margin"
            value="<%= make %>" name="make"></input>
    </td>
  </tr>                
  <tr>
    <td style="border-top: 0px; padding-top: 13px"><h4><small>Model</small></h4></td>
    <td style="border-top: 0px;" colspan=4>
      <input type="text" 
            class="inline-edit span table-input-margin"
            value="<%= model_name %>" 
            style="width: 349px"
            name="model_name"></input>
    </td>
  </tr>            
</tbody>
</table>
