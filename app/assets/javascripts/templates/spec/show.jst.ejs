<div class="container">

  <% if (!show_message || message == "" || message == null) { %>          
            
  <div class="company-banner-phone visible-phone clearfix">

      <% if (agent_phone) { %>
      
      <a class="btn btn-primary btn-large contact contact-phone pull-left"
          href="tel:<%= agent_phone %>">
          <strong>
          <i class="icon-phone icon-white"></i> <%= agent_name %>
          </strong>
      </a>
           
      <% } else { %>
      
      <a class="btn btn-primary btn-large contact contact-email" 
          href="mailto:<%= agent_email %>">
          <strong>
          <i class="icon-envelope-alt icon-white"></i> <%= agent_name %>
          </strong>
      </a>  
              
      <% } %>
        
  </div>
  
  <% } %>
      
  <div class="company-banner hidden-phone">

      <div style="float: left; position: relative; width: 100%;">
      
          <div class="agent-info">

            <h1>&nbsp;<%= agent_phone %> </h1>
            
            <h4><br><%= agent_name %></h4>
            
	          <% if (agent_website) { %>
            <h4>
            <a href="<%= agent_website %>">
            <%= agent_website.replace("http://", "") %>
            </a>
            </h4>
            <% } %>
            
          </div>      
          <% if (logo) { %>
          
            <% if (agent_website) { %>
                      
              <a href="<%= agent_website %>">
              <img class="company-logo has-website" src="<%= logo %>"></a>
              
            <% } else { %>
            
              <img class="company-logo" src="<%= logo %>">
              
            <% } %>
            
          <% } else { %>
          
          <h2 class="company-name"><%= agent_company %></h2>
          
          <% } %>

      </div>
      
  </div>

  <div id="retail-unit">

      <% if (show_message) { %>

      <% if (message != "" && message != null) { %>

      <div class="alert alert-info broker-message clearfix">

        <a class="close hidden-phone" id="close-message" data-dismiss="alert" href="#">x</a>
        
        <h2 class="alert-heading"><%= salutation %></h2>
        
        <p><%= message %></p>

        <span class="hidden-phone" id="dont-show-again"> 

            <input type="checkbox" 
                    id="dont-show-message"></input>   
                    
            Don't Show Again
            
        </span> 
              
        <div class="visible-phone clearfix">

            <% if (agent_phone) { %>
            
            <a class="btn btn-primary btn-large contact contact-phone pull-left"
                href="tel:<%= agent_phone %>">
                <strong>
                <i class="icon-phone icon-white"></i> <%= agent_name %>
                </strong>
            </a>
                 
            <% } else { %>
            
            <a class="btn btn-primary btn-large contact contact-email" 
                href="mailto:<%= agent_email %>">
                <strong>
                <i class="icon-envelope-alt icon-white"></i> <%= agent_name %>
                </strong>
            </a>  
                    
            <% } %>
              
        </div>
        
        <button class="pull-right btn-large close visible-phone" 
          id="close-message" 
          data-dismiss="alert">Close</button>        
        
      </div>

      <% } } %>

      <div class="row-fluid visible-desktop">
      
          <div class="span7">

              <h1><%= year %> <%= make %> <%= model_name %></h1>
              
              <% if (asking_price != null) { %>
              <p class="asking-price">Asking <%= asking_price %> </p>
              <% } else { %> <p> &nbsp;</p> <% } %>
              
              <a class="btn btn-primary btn-large contact contact-email" 
                  href="mailto:<%= agent_email %>">
                  <strong><i class="icon-envelope-alt icon-white"></i> <%= agent_name %></strong>
              </a>

              <ul class="hero-list">
                <% if (headline1 != null) { %>
                    <li><h3><%= headline1 %></h3></li>
                <% } %>
                <% if (headline2 != null) { %>
                    <li><h3><%= headline2 %></h3></li>
                <% } %>
                <% if (headline3 != null) { %>
                    <li><h3><%= headline3 %></h3></li>
                <% } %>        
              </ul>

          </div>

          <div class="span5" style="margin-bottom: 30px;">

              <!-- gallery items -->
              <ul class="gallery">
              
                  <% for (i in images) { %>
                  <% if (images[i].thumbnail == true) { %>
                  <li>
                    <a href="<%= images[i].original %>">
                      <div style="border-radius: 6px; position: absolute; margin: 5px; background: #222222; color: #EEEEEE">
                        <h1 style="margin: 10px 5px 5px 5px;"><i class="icon-search"></i></h1>
                      </div>
                      <img src="<%= images[i].original %>"
                        alt="<%= year %> <%= make %> <%= model_name %>"> 
                    </a>
                  </li>
                  <% } } %>
                                
                  <% for (i in images) { %>
                  <% if (images[i].thumbnail != true) { %>
                  <li>
                    <a href="<%= images[i].original %>">
                      <img src="<%= images[i].original %>"
                        alt="<%= year %> <%= make %> <%= model_name %>">  
                    </a>
                  </li>
                  <% } } %>
                  
                  <% if (images.length == 0) { %>
                    <img src="/assets/placeholder/aircraft_default_avatar_large.png" 
                      alt="<%= year %> <%= make %> <%= model_name %>"> 
                  <% } %>                  
                
              </ul>
              
          </div>
          
      </div>

      <div class="row-fluid visible-tablet">
          
          <h1><%= year %> <%= make %> <%= model_name %></h1>
          <% if (asking_price != null) { %>
          <p class="asking-price">Asking <%= asking_price %> </p>
          <% } else { %> <p> &nbsp;</p> <% } %>              
              
          <div class="span7" style="margin-left: 10px">
              
              <a class="btn btn-primary btn-large contact contact-email"
                  href="mailto:<%= agent_email %>">
                  <strong><i class="icon-envelope-alt icon-white"></i> <%= agent_name %></strong>
              </a>

              <ul class="hero-list" style="margin-bottom: 0px">
                <% if (headline1 != null) { %>
                    <li><h3><%= headline1 %></h3></li>
                <% } %>
                <% if (headline2 != null) { %>
                    <li><h3><%= headline2 %></h3></li>
                <% } %>
                <% if (headline3 != null) { %>
                    <li><h3><%= headline3 %></h3></li>
                <% } %>        
              </ul>

          </div>

          <div class="span5" style="margin-left: 0px; position: relative">

              <!-- gallery items -->
              <ul class="gallery">
              
                  <% for (i in images) { %>
                  <% if (images[i].thumbnail == true) { %>
                  <li>              
                    <a href="<%= images[i].original %>">
                          <div style="position: absolute; width: 100%">
                          <div style="border-radius: 6px; background: #222222; color: #EEEEEE; opacity: .8; width: 75px; height: 75px; margin: 60px auto;">
                            <h1 style="margin: 0px; padding: 20px 10px 0px 10px;"><i class="icon-search" style="opacity: .8;"></i></h1>
                          </div>
                          </div>                       
                      <img src="<%= images[i].original %>" 
                        alt="<%= year %> <%= make %> <%= model_name %>"> 
                    </a>
                  </li>
                  <% } } %>
                                
                  <% for (i in images) { %>
                  <% if (images[i].thumbnail != true) { %>
                  <li>
                    <a href="<%= images[i].original %>">
                      <img src="<%= images[i].original %>" 
                        alt="<%= year %> <%= make %> <%= model_name %>"> 
                    </a>
                  </li>
                  <% } } %>
                
                  <% if (images.length == 0) { %>
                    <img src="/assets/placeholder/aircraft_default_avatar_large.png" 
                      alt="<%= year %> <%= make %> <%= model_name %>"> 
                  <% } %>
                  
              </ul>

          </div>
      </div>
      
      <div class="row-fluid visible-phone">
          
          <div class="span4">
              <h1><%= year %> <%= make %> <%= model_name %></h1>
              <% if (asking_price != null) { %>
              <p class="asking-price">Asking <%= asking_price %> </p>
              <% } else { %> <p> &nbsp;</p> <% } %>              
          </div>
          
          <div class="span2" style="text-align: center; position: relative">
              <!-- gallery items -->
              <ul class="gallery" style="margin-bottom: 0px">
              
                  <% for (i in images) { %>
                  <% if (images[i].thumbnail == true) { %>
                  <li>                
                    <a href="<%= images[i].original %>">
                          <div style="position: absolute; width: 100%">
                          <div style="border-radius: 6px; background: #222222; color: #EEEEEE; opacity: .8; width: 75px; height: 55px; margin: 100px auto; padding-top: 20px;">
                            <h1 style="margin: 0px; padding: 0px;"><i class="icon-search" style="opacity: .8;"></i></h1>
                          </div>
                          </div>                      
                      <img src="<%= images[i].original %>"
                        alt="<%= year %> <%= make %> <%= model_name %>"> 
                    </a>
                  </li>
                  <% } } %>
                                
                  <% for (i in images) { %>
                  <% if (images[i].thumbnail != true) { %>
                  <li>
                    <a href="<%= images[i].original %>">
                      <img src="<%= images[i].original %>" 
                        alt="<%= year %> <%= make %> <%= model_name %>"> 
                    </a>
                  </li>
                  <% } } %>
                  
                  <% if (images.length == 0) { %>
                    <img src="/assets/placeholder/aircraft_default_avatar_large.png" 
                      alt="<%= year %> <%= make %> <%= model_name %>"> 
                  <% } %>
                                  
              </ul>
          </div>
          
          <ul class="hero-list" style="margin-top: 20px; margin-bottom: 20px">
            <% if (headline1 != null) { %>
                <li><h3><%= headline1 %></h3></li>
            <% } %>
            <% if (headline2 != null) { %>
                <li><h3><%= headline2 %></h3></li>
            <% } %>
            <% if (headline3 != null) { %>
                <li><h3><%= headline3 %></h3></li>
            <% } %>        
          </ul>
                  
      </div>

  </div>
  
  <div id="spec-detail">

          <div class="page-header">
            <h1>
              Detailed Specifications
            </h1>
          </div>

          <div class="row-fluid">
          
                <% if (engines.length == 0) { %>
                <div class="span12">
                <% } else { %>
                <div class="span6">
                <% } %>          

                  <table class="table table-striped table-bordered">
                    <tr><th colspan=2>General Information</th></tr>
                    
                    <% if (serial != null ) { %>
                    <tr>
                      <td><strong>Serial Number</strong></td>
                      <td><%= serial %></td>
                    </tr>
                    <% } %>
                    
                    <% if (registration != null ) { %>
                    <tr>
                      <td><strong>Registration</strong></td>
                      <td><%= registration %></td>
                    </tr>
                    <% } %>
                    
                    <% if (location != null && location != "") { %>
                    <tr>
                      <td><strong>Location</strong></td>
                      <td><%= location.city %>, <%= location.state %></td>
                    </tr>
                    <% } %>  
                     
                    <% if (tt != null && tt != "") { %>
                    <tr>
                      <td><strong>Total Time</strong></td>
                      <td><%= tt.formatMoney(0, ".", ",") %></td>
                    </tr>
                    <% } %>  
                                     
                    <% if (tc != null && tc != "") { %>
                    <tr>
                      <td><strong>Landings</strong></td>
                      <td><%= tc.formatMoney(0, ".", ",") %></td>
                    </tr>
                    <% } %> 
                             
                  </table>
                </div> <!-- end general -->

                <% if (engines.length > 0) { %>
                <div class="span6">
                  <table class="table table-striped table-bordered">
                    <tr>
                      <th>Engine Information</strong></th>
                      <% for (i in engines) { %>
                          <td><% if (engines[i].name != null) { %> <%= engines[i].name %> <% } %></td>
                      <% } %>
                    </tr>
                    
                    <tr>
                      <td><strong>Model Number</strong></td>
                      <% for (i in engines) { %>
                          <td><% if (engines[i].model_name != null) { %> <%= engines[i].model_name %> <% } %></td>
                      <% } %>
                    </tr>
                    
                    <tr>
                      <td><strong>Serial Number</strong></td>
                      <% for (i in engines) { %>
                          <td><% if (engines[i].serial != null) { %> <%= engines[i].serial %> <% } %></td>
                      <% } %>
                    </tr>
                    
                    <tr>
                      <td><strong>TTSN</strong></td>
                      <% for (i in engines) { %>
                          <td>
                              <% if (typeof(engines[i].tt) != "undefined" && engines[i].tt != null) { %> 
                                  <%= engines[i].tt.formatMoney(0, ".", ",") %> 
                              <% } %>
                          </td>
                      <% } %>
                    </tr>
                    
                    <tr>
                      <td><strong>SMOH</strong></td>
                      <% for (i in engines) { %>
                          <td>
                              <% if (typeof(engines[i].smoh) != "undefined" && engines[i].smoh != null) { %> 
                                  <%= engines[i].smoh.formatMoney(0, ".", ",") %> 
                              <% } %>
                          </td> 
                      <% } %>
                    </tr>
                    
                    <tr>
                      <td><strong>SHSI</strong></td>
                      <% for (i in engines) { %>
                          <td>
                              <% if (typeof(engines[i].shsi) != "undefined" && engines[i].shsi != null) { %> 
                                  <%= engines[i].shsi.formatMoney(0, ".", ",") %> 
                              <% } %>
                          </td>
                      <% } %>
                    </tr>
                  </table>
                </div> <!-- end engines -->
                <% } %>
                
          </div> <!-- end row -->

          <div class="row-fluid">

          <% if (avionics.length > 0) { %>

          <% if (equipment.length == 0) { %>
          <div class="span12">
          <% } else { %>
          <div class="span6">
          <% } %>

            <table class="hidden-phone table table-striped table-bordered">
              <tr><th colspan=4>Avionics</th></tr>
                <% for (i in avionics) { %>
                    <% if (i % 2 == 0) { %>
                        <tr>
                    <% } %>              
                    <td>
                        <% if (typeof(avionics[i].title) != "undefined") { %> 
                            <strong><%= avionics[i].title %></strong>
                        <% } %>
                    </td>
                    <td>
                        <% if (typeof(avionics[i].name) != "undefined") { %> 
                            <%= avionics[i].name %>
                        <% } %>
                    </td>    
                    <% if (i % 2 == 1) { %>
                        </tr>
                    <% } %>                            
                <% } %>                

                <% if (avionics.length % 2 == 1) { %>
                    <td></td><td></td></tr>
                <% } %>
            </table>
            <div class="visible-phone">
            <table class="table table-striped table-bordered">
              <tr><th colspan=4>Avionics</th></tr>
                <% for (i in avionics) { %>
                   
                    <tr>   
                    <td>
                        <% if (typeof(avionics[i].title) != "undefined") { %> 
                            <strong><%= avionics[i].title %></strong>
                        <% } %>
                    </td>
                    <td>
                        <% if (typeof(avionics[i].name) != "undefined") { %> 
                            <%= avionics[i].name %>
                        <% } %>
                    </td>    
                    </tr>
                                            
                <% } %>                
            </table>            
            </div>
          </div> <!-- end avionics -->
          <% } %>

          <% if (equipment.length > 0) { %>

          <% if (avionics.length == 0) { %>
          <div class="span12">
          <% } else { %>
          <div class="span6">
          <% } %>

            <table class="table table-striped table-bordered">
              <tr><th colspan=4>Other Features</th></tr>
                <% for (i in equipment) { %>
                    <% if (i % 2 == 0) { %>
                        <tr>
                    <% } %>              
                    <td>
                        <% if (typeof(equipment[i].name) != "undefined") { %> 
                            <%= equipment[i].name %>
                        <% } %>
                    </td>    
                    <% if (i % 2 == 1) { %>
                        </tr>
                    <% } %>                            
                <% } %>                

                <% if (equipment.length % 2 == 1) { %>
                    <td></td></tr>
                <% } %>
            </table>
          </div> <!-- end equipment -->
          <% } %>
      
     </div> <!-- end row -->
     
      <% if (description != null && description != "") { %>
      <div class="row-fluid">
      
          <div class="span12">
            <table class="table table-striped table-bordered">
              <tr><th>Description</th></tr>
              <tr>
                <td>
                  <%= description %>
                </td>
              </tr>
            </table>
          </div>
          
      </div>
      <% } %>

  </div> <!-- end spec-detail -->


  <div id="footer">
      Powered By <a href="http://www.jetdeck.co"><strong>JetDeck.co</strong></a>
  </div>

</div>




















