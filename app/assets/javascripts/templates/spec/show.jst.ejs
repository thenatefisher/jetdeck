<!-- CHAT MESSAGE --> 
<style>
.chat-fixed {
    position: fixed;
    bottom: -1000px;
    width: 100%;
    z-index: 99; 
}
.chat-window {
    width: 300px;
    background: white;
    color: #333;
    float: right;
    border: 1px solid #ccc;
    font-family: arial,sans-serif;
    font-size: 14px;
    line-height: 18px;
}
.chat-title {
    height: 20px;
    padding: 3px;
    background: #111;
    color: whiteSmoke;
    font-weight: bold;
}
.chat-title-close {
    float: right; 
    margin-right: 5px; 
    font-size: 80%; 
    cursor: pointer;
}
.chat-title-status-icon {
    margin-left: 5px; 
    margin-right: 10px;
    color: #FE9A2E; 
    font-size: 80%;
}
.chat-message {

}
.chat-message > div {
    margin: 10px;
    margin-bottom: 115px;
    color: black;
}
.chat-reply {
    height: 50px;
    position: absolute;
    bottom: 0px;
}
.chat-reply-button {
    height: 44px;
    width: 287px;
    margin: 0px 6px 12px 6px;
    font-weight: bold;
    color: #555555;
}
</style>

<script>
$(document).ready(function(){
    window.setTimeout(showChat, 4000);
    $(".chat-title-close").click(function(){
        $(".chat-fixed").animate({bottom:'-1000px'}, 800);
    });
});
function showChat() {
    $(".chat-fixed").animate({bottom:'0px'}, 800);
}
</script>

<div class="chat-fixed">
    <div class="container">
        <div class="chat-window">
            <div class="chat-title">
                <span class="chat-title-close">X</span>
                <span style="float: left">
                    <i class="icon-circle chat-title-status-icon"></i> <%= agent_name %>
                </span>
            </div>
            <div class="chat-message">
                <div>
                    <strong><%= agent_first %>: </strong> &nbsp;&nbsp; <%= message %>
                    <!--<br><span class="muted">Sent at 4:32pm on May 3</span>-->
                </div>
            </div>
            <div class="chat-reply">
                <button class="btn chat-reply-button"><i class="icon-reply"></i> &nbsp; Reply to <%= agent_first %></button>                
            </div>
        </div>
    </div>
</div>
<% } %>