<!-- GALLERY -->
<!-- SlidesJS Optional: If you'd like to use this design -->
<style>

    #slides {
      display: none
    }
    
    #slides .slidesjs-navigation,
    .slidesjs-navigation,
    .slidesjs-stop.slidesjs-navigation, 
    .slidesjs-play.slidesjs-navigation {
        position: absolute;
        color: white;
        font-weight: bold;
        z-index: 99;
        margin-top: -30px;
    }    

    #slides .slidesjs-previous {
      margin-right: 5px;
      float: left;
    }

    #slides .slidesjs-next {
      margin-right: 5px;
      float: left;
    }

    .slidesjs-pagination {
      margin: 6px 0 0;
      float: right;
      list-style: none;
    }

    .slidesjs-pagination li {
      float: left;
      margin: 0 1px;
    }

    .slidesjs-pagination li a {
      display: block;
      width: 13px;
      height: 0;
      padding-top: 13px;
      background-image: url(http://www.slidesjs.com/img/pagination.png);
      background-position: 0 0;
      float: left;
      overflow: hidden;
    }

    .slidesjs-pagination li a.active,
    .slidesjs-pagination li a:hover.active {
      background-position: 0 -13px
    }

    .slidesjs-pagination li a:hover {
      background-position: 0 -26px
    }

    #slides a:link,
    #slides a:visited {
      color: white;
    }

    #slides a:hover,
    #slides a:active {
      color: #006dcc;
      text-decoration: none;
    }

    .navbar {
      overflow: hidden
    }

</style>
<!-- SlidesJS Required: -->

<script>
    $(function(){
      $("#slides").slidesjs({
        height: 250,
        pagination: {active: false}, 
        navigation: {active: false},
        play: {
            active: true,
            auto: true
        }, 
        effect: {
            slide: {speed: 1000}
        }
      });

    container_margin_left = $(".container").css("marginLeft");

    $(".slidesjs-navigation").css("marginLeft", container_margin_left);
    $(".slidesjs-stop").html("<i class='icon-stop'></i> Stop Slideshow");
    $(".slidesjs-play").html("<i class='icon-play'></i> Play Slideshow");

    });
</script>

<div id="slides">
    <% for (var i = 0; i < images.length; i++) { %>
        <img src="<%=images[i].slides%>">
    <% } %>  
</div>

<!-- SPEC NAV -->
<style> 
    .contact-button {
        vertical-align: middle;
        line-height: 45px;
        text-align: center;
        float: left;
        padding: 0px 20px;
        cursor: pointer;
        border-bottom: 2px transparent solid;
        float:right;
        padding-right: 0px;
    }
    .contact-button .btn-primary{
        background-color: #006dcc;
        background-image: none;
    }
    .container.spec-nav {
        border-bottom: 1px #DDD solid;
    }

    .nav-tabs {
        border-bottom: 0px solid #ddd;
    }
    .spec-nav .nav {
        margin-left: 0;
        margin-bottom: 0px;
        margin-left: 20px;
        list-style: none;
    }

    .nav-tabs > li {
        margin-bottom: -2px;
    }
    .nav-tabs > li, .nav-pills > li {
        float: left;
    }
    li {
        line-height: 20px;
    }

    .nav-tabs > li > a {
        padding: 0px 20px;
        line-height: 45px;
        border: 2px solid transparent;
        -webkit-border-radius: 0;
        -moz-border-radius: 0;
        border-radius: 0;
        color: #333333;
    }

    .nav-tabs > .active > a, .nav-tabs > .active > a:hover, .nav-tabs > .active > a:focus {
        color: #555555;
        background-color: transparent;
        border: 0px solid #ddd;
