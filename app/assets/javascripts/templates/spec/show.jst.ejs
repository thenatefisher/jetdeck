<div class="container">

  <div class="row company-banner">
    <div class="span12 whitebox">
    
      <% if (logo) { %>
      
        <% if (agent_website) { %>
                  
          <a href="<%= agent_website %>">
          <img class="company-logo" style="margin-top: 20px;" src="<%= logo %>"></a>
          
        <% } else { %>
        
          <img class="company-logo" src="<%= logo %>">
          
        <% } %>
        
      <% } else { %>
      
      <h2 class="company-name"><%= agent_company %></h2>
      
      <% } %>
      
      <div class="pull-right" style="margin: 10px 20px; text-align: right">
        <h1>&nbsp;<%= agent_phone %> </h1>
        <h4>&nbsp;</h4>
        <h4><%= agent_name %></h4>
	<% if (agent_website) { %>
        <h4><a href="<%= agent_website %>"><%= agent_website.replace("http://", "") %></a></h4><% } %>
      </div>
      <div style="clear: both"></div>
    </div>
  </div>

  <div class="hero-unit" style="background: white; padding: 30px; padding-bottom: 0px">

    <% if (show_message) { %>
    <% if (message != "" && message != null) { %>
    <div class="alert alert-info broker-message">
    
      <a class="close" id="close-message" data-dismiss="alert" href="#">×</a>
      
      <h2 class="alert-heading"><%= salutation %></h2>
      
      <p><%= message %></p>
   
      <span style="float: right; margin-top: 7px; margin-right: -15px;"> 
      
          <input type="checkbox" 
                  id="dont-show-message" 
                  style="margin-right: 5px; margin-top: 0px"></input>   
                  
          Don't Show Again</span> 
      
      <div style="clear: both"></div>
      
    </div>
    <% } } %>

    <div class="span5 hero-text-data">
      <h1><%= year %> <%= make %> <%= model_name %></h1>
      <% if (asking_price != null) { %>
      <p>Asking <%= asking_price %> </p>
      <% } else { %> <p> &nbsp;</p> <% } %>
      <br>
      <p>
        <button class="btn btn-primary btn-large" id="message-seller">
          <strong><i class="icon-envelope-alt icon-white"></i> <%= agent_name %></strong>
        </button>
      </p>

      <ul class="hero-list">
        <% if (headline1 != null) { %>
            <li><h3><i class="icon-caret-right" style="margin: 4px 5px 0 0"></i> <%= headline1 %></h3></li>
        <% } %>
        <% if (headline2 != null) { %>
            <li><h3><i class="icon-caret-right" style="margin: 4px 5px 0 0"></i> <%= headline2 %></h3></li>
        <% } %>
        <% if (headline3 != null) { %>
            <li><h3><i class="icon-caret-right" style="margin: 4px 5px 0 0"></i> <%= headline3 %></h3></li>
        <% } %>        
      </ul>

    </div>

    <div class="hero-image-float">

      <div id="hero-carousel" class="carousel slide">
        <!-- Carousel items -->
        <div class="carousel-inner">
          <% for (i in images) { %>
          <div class="item<% if (images[i].thumbnail == true) { %> active<% } %>">
            <img class="hero-image" 
                  src="<%= images[i].preview %>" 
                  data-spec_lightbox="<%= images[i].spec_lightbox %>"
                  data-original="<%= images[i].original %>"/>
          </div>
          <% } %>
        </div>
        <!-- Carousel nav -->
        <a class="carousel-control left hide" href="#hero-carousel" data-slide="prev">&lsaquo;</a>
        <a class="carousel-control right hide" href="#hero-carousel" data-slide="next">&rsaquo;</a>
      </div>

    </div>

    <div style="clear: both"></div>

  </div>

  <div class="row">
    <div class="span12 whitebox">

      <div class="page-header" style="margin-left: 20px; margin-right: 20px;">
        <h1>
          Detailed Specifications
          <!-- <span class="pull-right"><button class="btn btn-small print-spec"><i class="icon-print"></i> Print</button></span> -->
        </h1>
      </div>

      <div class="span4 spec-data">
        <table class="table table-striped table-bordered ">
          <tr><th colspan=2>General Information</th></tr>
          <% if (serial != null ) { %>
          <tr>
            <td><strong>Serial Number</strong></td>
            <td><%= serial %></td>
          </tr>
          <% } %>
          <% if (registration != null ) { %>
          <tr>
            <td><strong>Registration</strong></td>
            <td><%= registration %></td>
          </tr>
          <% } %>
          <% if (location != null && location != "") { %>
          <tr>
            <td><strong>Location</strong></td>
            <td><%= location.city %>, <%= location.state %></td>
          </tr>
          <% } %>   
          <% if (tt != null && tt != "") { %>
          <tr>
            <td><strong>Total Time</strong></td>
            <td><%= tt.formatMoney(0, ".", ",") %></td>
          </tr>
          <% } %>                   
          <% if (tc != null && tc != "") { %>
          <tr>
            <td><strong>Landings</strong></td>
            <td><%= tc.formatMoney(0, ".", ",") %></td>
          </tr>
          <% } %>          
        </table>
      </div> <!-- end table wrapper div -->
      
      <div class="offset4 spec-data" style="margin-right: 20px;">
        <table class="table table-striped table-bordered ">
          <tr>
            <th>Engine Information</strong></th>
            <% for (i in engines) { %>
                <td><% if (engines[i].name != null) { %> <%= engines[i].name %> <% } %></td>
            <% } %>
          </tr>
          
          <tr>
            <td><strong>Model Number</strong></td>
            <% for (i in engines) { %>
                <td><% if (engines[i].model_name != null) { %> <%= engines[i].model_name %> <% } %></td>
            <% } %>
          </tr>
          <tr>
            <td><strong>Serial Number</strong></td>
            <% for (i in engines) { %>
                <td><% if (engines[i].serial != null) { %> <%= engines[i].serial %> <% } %></td>
            <% } %>
          </tr>
          <tr>
            <td><strong>TTSN</strong></td>
            <% for (i in engines) { %>
                <td>
                    <% if (typeof(engines[i].tt) != "undefined" && typeof(engines[i].tt) != null) { %> 
                        <%= engines[i].tt.formatMoney(0, ".", ",") %> 
                    <% } %>
                </td>
            <% } %>
          </tr>
          <tr>
            <td><strong>SMOH</strong></td>
            <% for (i in engines) { %>
                <td>
                    <% if (typeof(engines[i].smoh) != "undefined" && typeof(engines[i].smoh) != null) { %> 
                        <%= engines[i].smoh.formatMoney(0, ".", ",") %> 
                    <% } %>
                </td> 
            <% } %>
          </tr>
          <tr>
            <td><strong>SHSI</strong></td>
            <% for (i in engines) { %>
                <td>
                    <% if (typeof(engines[i].shsi) != "undefined" && typeof(engines[i].shsi) != null) { %> 
                        <%= engines[i].shsi.formatMoney(0, ".", ",") %> 
                    <% } %>
                </td>
            <% } %>
          </tr>
        </table>
      </div> <!-- end table wrapper div -->

      <% if (avionics.length > 0) { %>
      <div class="span6 spec-data">
        <table class="table table-striped table-bordered ">
          <tr><th colspan=4>Avionics</th></tr>
            <% for (i in avionics) { %>
                <% if (i % 2 == 0) { %>
                    <tr>
                <% } %>              
                <td>
                    <% if (typeof(avionics[i].title) != "undefined") { %> 
                        <strong><%= avionics[i].title %></strong>
                    <% } %>
                </td>
                <td>
                    <% if (typeof(avionics[i].name) != "undefined") { %> 
                        <%= avionics[i].name %>
                    <% } %>
                </td>    
                <% if (i % 2 == 1) { %>
                    </tr>
                <% } %>                            
            <% } %>                

            <% if (avionics.length % 2 == 1) { %>
                <td></td><td></td></tr>
            <% } %>
        </table>
      </div> <!-- end table wrapper div -->
      <% } %>
      
      <% if (equipment.length > 0) { %>
      
      <% if (avionics.length == 0) { %>
      <div class="spec-data" style="margin-left: 30px; margin-right: 20px;">
      <% } else { %>
      <div class="offset6 spec-data" style="margin-right: 20px;">
      <% } %>
      
        <table class="table table-striped table-bordered ">
          <tr><th colspan=4>Other Features</th></tr>
            <% for (i in equipment) { %>
                <% if (i % 2 == 0) { %>
                    <tr>
                <% } %>              
                <td>
                    <% if (typeof(equipment[i].name) != "undefined") { %> 
                        <%= equipment[i].name %>
                    <% } %>
                </td>    
                <% if (i % 2 == 1) { %>
                    </tr>
                <% } %>                            
            <% } %>                

            <% if (equipment.length % 2 == 1) { %>
                <td></td></tr>
            <% } %>
        </table>
      </div> <!-- end table wrapper div -->
      <% } %>
     
      <% if (description != null && description != "") { %>
      <div style="margin-left: 30px; margin-right: 20px;">
        <table class="table table-striped table-bordered">
          <tr><th>Description</th></tr>
          <tr>
            <td>
              <%= description %>
            </td>
          </tr>
        </table>
      </div>
      <% } %>
      
      <div style="clear: both"></div>

    </div>  <!-- end .whitebox -->

  </div> <!-- end .row -->


  <div class="pull-right" style="margin: 20px;">

      Powered By <a href="http://www.jetdeck.co"><strong>JetDeck.co</strong></a>

  </div>

</div>

<div class="modal hide" id="image-modal" style="width: 650px">
  <div class="modal-body" style="max-height: 650px; text-align: center;"></div>
  <div class="modal-footer">
    <button class="btn" data-dismiss="modal">Close</button>
  </div>
</div>

<div class="modal hide" id="message-modal">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal">×</button>
    <h3>
        Re: <%= year %> <%= make %> <%= model_name %> 
        <% if (serial != null && serial != "") { %> S/N <%= serial %> <% } %>
    </h3>
  </div>
  <div class="modal-body">

        <div>
        
          <div class="span1" style="padding-top: 4px;"><strong>From:</strong></div>
          <div class="span4">
            <input type="text" id="to-field" style="width: 100%" placeholder="<%= recipient %>">
          </div>
          
          <div style="clear:both; margin-top: 10px;"></div>

          <div class="span1" style="padding-top: 4px;"><strong>To:</strong></div>
          <div class="span4">
            <input type="text" 
                   id="from-field" 
                   style="width: 100%" 
                   disabled 
                   placeholder="<%= agent_name %> <<%= agent_email %>>">
          </div>
          
          <div class="span5">
            <textarea class="input-xlarge" 
                        style="width: 100%; margin-top: 10px;" 
                        id="textarea" 
                        rows="6"></textarea>
          </div>
        </div>
        
  </div>
  <div class="modal-footer">
    <a href="#" class="btn" data-dismiss="modal">Close</a>
    <a href="#" class="btn btn-primary"><i class="icon-envelope icon-white"></i> Send</a>
  </div>
</div>
