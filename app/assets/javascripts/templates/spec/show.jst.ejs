<!-- SPEC NAV -->
<style> 
    .contact-button {
        vertical-align: middle;
        line-height: 45px;
        text-align: center;
        float: left;
        padding: 0px 20px;
        cursor: pointer;
        border-bottom: 2px transparent solid;
        float:right;
        padding-right: 0px;
    }
    .contact-button .btn-primary{
        background-color: #006dcc;
    }
    .container.spec-nav {
        border-bottom: 1px #DDD solid;
    }

    .nav-tabs {
        border-bottom: 0px solid #ddd;
    }
    .nav {
        margin-left: 0;
        margin-bottom: 0px;
        margin-left: 30px;
        list-style: none;
    }

    .nav-tabs > li {
        margin-bottom: -2px;
    }
    .nav-tabs > li, .nav-pills > li {
        float: left;
    }
    li {
