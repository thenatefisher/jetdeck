<div class="row" style="margin-left: 20px;">

    <div style="margin-left: 20px;">
        <div class="span9 page-header">
            <h1>All Contacts <small> (<%= count %>)</small></h1>        
        </div>
    </div>

 
    <% if (count > 0) { %>
    <div class="span9">
    <table id="contacts" class="table table-condensed">       
        <thead>
            <tr>
            <th>First</th>
            <th>Last</th>
            <th>Email</th>
            <th>Company</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
    </div>
 
    <% } else { %>
    <div class="span8 alert alert-info">
        <br/><strong>Welcome to JetDeck!</strong><br/>
        You don't have any contacts yet. Create a new contact by clicking the 'New Contact' button.
        <br/><br/>
    </div> 
    <% } %>   
       
</div>

<% if (pages > 1) { %>
<div class="row"  style="margin-left: 20px;" >
    <div class="span9">
        <div class="pagination pull-right">
          <ul>
            <li><a class="prev"  onclick="return false;" href="#">Prev</a></li>

            <% if (pages > 2) { %>
            <% for (var i = 1; i < pages + 1; i++) { %>
            <li><a class="page" rel="<%= i %>" onclick="return false;" href="#"><%= i %></a></li>
            <% } } %>

            <li><a class="next" onclick="return false;" href="#">Next</a></li>
          </ul>
        </div>
    </div>
</div>
<% } %>
