  <div class="offset2">

       <div id="profile_name_edit" class="">
           <input type="text"
                    class="inline-edit profile_field span3"
                    name="first"
                    value="<%= contact.first %>"
                    placeholder="[First Name]">
           </input>
           <input type="text"
                    class="inline-edit profile_field span3"
                    name="last"
                    value="<%= contact.last %>"
                    placeholder="[Last Name]">
           </input>
       </div>

       <input type="text"
                class="inline-edit profile_field span6"
                value="<%= contact.company %>"
                name="company"
                placeholder="[Enter Company Name]">
       </input>

      <div class="row">
        <div class="span8">
          <table class="profile-header-details table-condensed">
              <tbody>
                  <tr>
                      <td>
                          <div class="profile-header-label span3">Account Type</div>
                      </td>
                      <td>
                          Beta / Trial <small class="hide"><a style="margin-left: 20px;" href="#">Upgrade</a></small>
                      </td>
                  </tr>
                  <tr>
                      <td>
                          <div class="profile-header-label span3">Email Address</div>
                      </td>
                      <td>
                          <input  type="text"
                                  class="inline-edit profile_field span4"
                                  value="<%= contact.email %>"
                                  name="email"
                                  placeholder="[Enter Email Address]">
                          </input>
                      </td>
                  </tr>
                  <tr>
                      <td>
                          <div class="profile-header-label span3">Email Settings</div>
                      </td>
                      <td>
                          <span><input type="checkbox"></input> Receive New Feature Announcements </span><br>
                          <span><input type="checkbox"></input> Receive Messages from Other Users </span><br>
                          <span><input type="checkbox"></input> Receive JetDeck Newsletter </span>
                      </td>
                  </tr>
                  <tr>
                      <td>
                          <div class="profile-header-label span3">Change Password</div>
                      </td>
                      <td>
                          <input  type="text"
                                  class="profile_field span4"
                                  value=""
                                  name=""
                                  placeholder="[Old Password]">
